<Page x:Class="Marathon_skills_2016.Pages.PageIW"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Marathon_skills_2016.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="PageIW">

    <Grid>
        <StackPanel Orientation="Vertical" 
                    VerticalAlignment="Center" >
            <Button Name="BtnIU" Width="150" 
                    Height="30" Margin="5" 
                    VerticalAlignment="Center"
                    Content="Я учавствовал ранее" 
                    Click="BtnIU_Click" FontSize="14" 
                    FontFamily="Veranda"/>
            <Button Name="BtnIN" Content="Я новый участник" 
                    Width="150" 
                    Height="30" Margin="5" 
                    VerticalAlignment="Center"
                    Click="BtnIN_Click" FontSize="14" 
                    FontFamily="Veranda"/>
        </StackPanel>
    </Grid>
</Page>
